import matplotlib.pyplot as plt
fig, ax = plt.subplot as plt
#Corona Virus Pandemic Information in US until 03/30/2020 02:23 AM
data = {
    "Coronavirus Cases": 142735,
    "Deaths": 2489,
    "Recovered": 4562
    
}
#x & y axis value keys:
x_axis_data = data.keys()
y_axis_data = data.values()

#Plot a bar graph:
ax.bar(x_axis_data,y_axis_data)

#naming axes on a graph:
ax.set(ylim = [0, 150000],
ylabel='Coronavirus Outbreak information',
xlabel='Number of people',
title= 'Corona Virus Pandemic Information in US')

#Displaying the graph:
plt.show()
